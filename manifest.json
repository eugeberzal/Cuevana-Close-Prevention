{
	"name": "Cuevana Close Prevention Popup",
	"manifest_version": 2,
	"version": "1.2.1",
	"description": "__MSG_extensionDescription__",
	"icons": { "128": "img/icon128.png" },
	"permissions": ["tabs", "http://www.cuevana.tv/*"],
	"content_scripts": [
	    {
	      "matches": ["http://www.cuevana.tv/*"],
	      "js": ["js/main.js"]
	    }
	  ],
	"options_page": "options.html",
	"default_locale": "es"
}